{
  "name": "nutritionist-gpt",
  "version": "0.0.1",
  "description": "NutritionistGpt NestJs Service",
  "main": "dist/main.js",
  "author": "",
  "scripts": {
    "build": "rimraf ./dist && nest build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "validate": "run-p --print-label lint test build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/infrastructure/persistence/ormconfig.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create ./src/infrastructure/persistence/migrations/migration",
    "migration:generate": "npm run typeorm migration:generate ./src/infrastructure/persistence/migrations/migration",
    "migration:run": "npm run typeorm migration:run -d ./src/infrastructure/persistence/ormconfig.ts",
    "migration:revert": "npm run typeorm migration:revert -d ./src/infrastructure/persistence/ormconfig.ts"
  },
  "dependencies": {
    "@nestjs/cli": "^10.2.1",
    "@langchain/community": "^0.0.54",
    "@langchain/openai": "^0.0.28",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/typeorm": "^10.0.0",
    "ai": "^3.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "langchain": "^0.1.36",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "qs": "^6.10.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.8",
    "@types/express": "^4.17.20",
    "@types/node": "^20.11.21",
    "@types/passport-jwt": "^4.0.1",
    "@types/qs": "^6.9.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
